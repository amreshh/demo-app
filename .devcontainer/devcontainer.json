{
  "image": "docker.io/maven:3.8.1-openjdk-17-slim",
  "remoteEnv": {
      "MAVEN_OPTS": "-Dmaven.repo.local=./.mvn/repository",
      "DEVOTEAM": "https://nl.devoteam.com/",
      "ITECH": "https://nl.devoteam.com/expertise/innovative-tech/",
      "EVENTS": "https://nl.devoteam.com/event/",
      "CAREERS": "https://nl.devoteam.com/working-at-devoteam/",
      "INSIGHTS": "https://nl.devoteam.com/insights-events/"
  },
  "runArgs": [
    "--network=host"
  ],
  "shutdownAction": "stopContainer",
  "extensions": [
      "CoenraadS.bracket-pair-colorizer-2",
      "mhutchie.git-graph",
      "eamodio.gitlens",
      "redhat.vscode-xml",
      "redhat.java",
      "VisualStudioExptTeam.vscodeintellicode",
      "vscjava.vscode-java-debug",
      "vscjava.vscode-maven",
      "vscjava.vscode-java-test",
      "vscjava.vscode-java-dependency",
      "redhat.fabric8-analytics",
      "HCLTechnologies.hclappscancodesweep"
  ]
}